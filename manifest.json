{
  "manifest_version": 3,
  "name": "AutoTab Extension",
  "version": "1.0",
  "description": "Cambia automáticamente entre pestañas con un intervalo configurable.",
  "permissions": ["tabs", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "AutoTab"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
} 